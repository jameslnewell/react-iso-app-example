{
  "name": "react-iso-app-example",
  "dependencies": {
    "babel": "^5.8.21",
    "express": "^4.13.3",
    "iso-app": "iso-js/app",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babelify": "^6.2.0",
    "browserify": "^11.0.1",
    "nodemon": "^1.4.1",
    "watchify": "^3.3.1"
  },
  "scripts": {
    "server.watch": "nodemon ./lib/server.js",
    "client.build": "browserify -t babelify ./lib/client.js -o ./public/index.js",
    "client.watch": "watchify -t babelify ./lib/client.js -o ./public/index.js"
  }
}
